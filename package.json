{
  "name": "hono-pino",
  "version": "0.0.4",
  "description": "A pino logger plugin for hono",
  "license": "MIT",
  "author": "maou-shonen",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "hono",
    "pino",
    "logger"
  ],
  "homepage": "https://github.com/maou-shonen/hono-pino#readme",
  "bugs": {
    "url": "https://github.com/maou-shonen/hono-pino/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "prepare": "husky",
    "build": "tsup-node",
    "format": "prettier --write .",
    "lint": "eslint --quiet ."
  },
  "dependencies": {
    "deepmerge": "^4.3.1",
    "pino": "^9.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "@types/node": "^20.14.2",
    "esbuild": "^0.21.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-unicorn": "^54.0.0",
    "hono": "^4.4.6",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "pino-test": "^1.0.1",
    "prettier": "^3.3.2",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.13.0",
    "vitest": "^1.6.0"
  }
}
